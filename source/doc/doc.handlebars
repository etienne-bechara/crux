<!DOCTYPE html>
<html>

<head>
  <title>{{ title }}</title>

  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1">

  {{#if favicon}}
  <link rel="shortcut icon" type="image/x-icon" href="{{ favicon }}" />
  {{/if}}

  <style>
    body {
      margin: 0;
      padding: 0;
    }

    * {
      scrollbar-width: auto;
      scrollbar-color: #192226 #263238;
    }

    *::-webkit-scrollbar {
      width: 10px;
    }

    *::-webkit-scrollbar-track {
      background: #263238;
    }

    *::-webkit-scrollbar-thumb {
      background-color: #192226;
      border-radius: 0px;
      border: 0px none #ffffff;
    }
  </style>
</head>

<body>
  <div id="redoc-container"></div>
  <script src="https://cdn.jsdelivr.net/npm/redoc@2.0.0-rc.66/bundles/redoc.standalone.js"> </script>
  <script src="https://cdn.jsdelivr.net/gh/wll8/redoc-try@1.4.1/dist/try.js"></script>
  <script>
    const currentUrl = window.location.href.split(/[?#]/)[0];
    const currentOpenApiUrl = currentUrl.replace(/\/docs$/, '/openapi/json');

    initTry({
      openApi: '{{ openApiUrl }}' || currentOpenApiUrl,
      tryText: 'â¯ˆ',
      redocOptions: JSON.parse('{{{ options }}}'),
    });
  </script>
</body>

</html>
